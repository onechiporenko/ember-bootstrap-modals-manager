{
  "name": "ember-bootstrap-modals-manager",
  "version": "7.0.0",
  "description": "Modals-manager for modals from ember-bootstrap",
  "keywords": [
    "bootstrap",
    "modal",
    "ember-addon"
  ],
  "repository": "https://github.com/onechiporenko/ember-bootstrap-modals-manager",
